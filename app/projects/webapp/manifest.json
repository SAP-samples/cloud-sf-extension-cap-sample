{
	"_version": "1.15.0",
	"sap.app": {
		"id": "projects",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"ach": "CA-UI5-FE",
		"dataSources": {
			"AdminService": {
				"uri": "/admin/",
				"type": "OData",
				"settings": {
					"odataVersion": "4.0"
				}
			}
		},
		"offline": false,
		"resources": "resources.json",
		"sourceTemplate": {
			"id": "ui5template.fiorielements.v4.lrop",
			"version": "1.0.0"
		},
		"crossNavigation": {
			"inbounds": {
				"intent1": {
					"signature": {
						"parameters": {},
						"additionalParameters": "allowed"
					},
					"semanticObject": "projects",
					"action": "display",
					"title": "Projects",
					"subTitle": "List of Projects",
					"icon": "sap-icon://project-definition-triangle"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"resources": {
			"js": [],
			"css": []
		},
		"dependencies": {
			"minUI5Version": "1.72.0",
			"libs": {
				"sap.fe.templates": {},
				"sap.ui.fl": {}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"uri": "i18n/i18n.properties"
			},
			"": {
				"dataSource": "AdminService",
				"settings": {
					"synchronizationMode": "None",
					"operationMode": "Server",
					"autoExpandSelect": true,
					"earlyRequests": true,
					"groupProperties": {
						"default": {
							"submit": "Auto"
						}
					}
				}
			}
		},
		"routing": {
			"routes": [{
				"pattern": ":?query:",
				"name": "ProjectList",
				"target": "ProjectList"
			}, {
				"pattern": "Project({key}):?query:",
				"name": "ProjectObjectPage",
				"target": "ProjectObjectPage"
			}, {
				"pattern": "Project({key})/employees({key2}):?query:",
				"name": "MappingsObjectPage",
				"target": "MappingsObjectPage"
			}],
			"targets": {
				"ProjectList": {
					"type": "Component",
					"id": "ProjectList",
					"name": "sap.fe.templates.ListReport",
					"options": {
						"settings": {
							"entitySet": "Project",
							"variantManagement": "Page",
							"initialLoad": true,
							"navigation": {
								"Project": {
									"detail": {
										"route": "ProjectObjectPage"
									}
								}
							},
							"controlConfiguration": {
								"@com.sap.vocabularies.UI.v1.LineItem": {
									"tableSettings": {
										"type": "ResponsiveTable",
										"enableExport": false,
										"selectionMode": "Single"
									}
								}
							}
						}
					}
				},
				"ProjectObjectPage": {
					"type": "Component",
					"id": "ProjectObjectPage",
					"name": "sap.fe.templates.ObjectPage",
					"options": {
						"settings": {
							"entitySet": "Project",
							"editableHeaderContent": false,
							"controlConfiguration": {
								"employees/@com.sap.vocabularies.UI.v1.LineItem": {
									"tableSettings": {
										"type": "ResponsiveTable",
										"enableExport": false,
										"enablePaste": false,
										"creationMode": {
											"name": "Inline",
											"createAtEnd": true
										}
									}
								}
							}
						}
					}
				},
				"MappingsObjectPage": {
					"type": "Component",
					"id": "MappingsObjectPage",
					"name": "sap.fe.templates.ObjectPage",
					"options": {
						"settings": {
							"editableHeaderContent": false,
							"entitySet": "Mappings"
						}
					}
				}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		}
	},
	"sap.platform.abap": {
		"_version": "1.1.0",
		"uri": ""
	},
      "sap.cloud": {
        "public": true,
        "service": "sap.sf"
    },
	"sap.platform.hcp": {
		"_version": "1.1.0",
		"uri": ""
	},
	"sap.fiori": {
		"_version": "1.1.0",
		"registrationIds": [],
		"archeType": "transactional"
	}
}